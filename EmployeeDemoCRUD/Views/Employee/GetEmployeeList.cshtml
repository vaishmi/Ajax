@using EmployeeDemoCRUD.Models
@model IEnumerable<Employee>


<h2>Employee</h2>
<table border="1">
    <tr>
        <th>Id</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Joining Date</th>
        <th>Department Name</th>
    </tr>

    @foreach (Employee e in Model)
    {
        <tr>
            <td>@e.Id</td>
            <td>@e.FirstName</td>
            <td>@e.LastName</td>
            <td>@e.JoiningDate</td>
            <td>@e.DeptName</td>
        </tr>
    }


</table>

